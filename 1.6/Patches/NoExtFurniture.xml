<?xml version="1.0" encoding="UTF-8"?>
<Patch>
   <Operation Class="PatchOperationReplace">
      <!-- There may be no mods using this designation category, so don't error if there's none. -->
      <success>Always</success>
      <!-- Replace any "Ext. Furniture" designation categories with normal "Furniture" designation category. -->
      <xpath>Defs/ThingDef[designationCategory="AOMorefurniture"]/designationCategory</xpath>
      <value>
         <designationCategory>Furniture</designationCategory>
      </value>
   </Operation>
</Patch>
